<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">
  <title>Precision School Improvement</title>
  <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
  <% if false %>
  <link rel="stylesheet" href="/resources/demos/style.css">
      <% end %>
  <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  <script>
    $( function() {
      $( ".datepicker" ).datepicker();
      $( ".format" ).on( "change", function() {
        $( ".datepicker" ).datepicker( "option", "dateFormat", $( this ).val() );
      });
    } );
  </script>
  <script>
    $( function() {
      $( ".accordion" ).accordion();
    } );
  </script>
  <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <%= javascript_include_tag "https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js", "https://oss.maxcdn.com/respond/1.4.2/respond.min.js" %>
  <![endif]-->

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag("#{STRIPE_JS_HOST}/v2/#{STRIPE_JS_FILE}") %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script>
    Stripe.setPublishableKey(
      "<%= Rails.application.secrets.stripe_publishable_key %>");
  </script>

  <%= csrf_meta_tags %>
</head>
<body>
  <div class="main_header">
    <div class="main_background_image">
      <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
          <div class="marque">
            <%= render 'layouts/marque_bar' %>
          </div>
        </div>
        <div class="col-md-3"></div>
        <%= render '/session/contact_us_modal' %>
        <%= render '/session/career_modal' %>
      </div>
    </div>
  </div>
  <div class="page_backgroundx">
    <div class="precision_container">
      <%#= render 'layouts/nav_bar' %>
      <div class="row site-frame">
        <% if !flash.empty? && !@no_flash %><%#= render 'layouts/flash_bar' %>
        <% end %>
        <%#= render 'layouts/ajax_loader'%>
        <%= yield %>
      </div>
    </div>
    <%= render 'layouts/main_footer' %>
  </div>
<!-- /.container -->

<!-- jQuery -->
    <% if false %>
<script src="/js/jquery.js"></script>

<!-- Bootstrap Core JavaScript -->
<script src="/js/bootstrap.min.js"></script>
<% end %>
<!-- Script to Activate the Carousel -->
<script>
    $('.carousel').carousel({
        interval: 5000 //changes the speed
    })
</script>
<script>
  $('form#id').submit(function(){
    $(this).find(':input[type=submit]').prop('disabled', true);
  });
</script>
<script>
  $('form#payment-form').submit(function(){
    $(this).find(':input[type=submit]').prop('disabled', true);
  });
</script>

</body>
</html>
