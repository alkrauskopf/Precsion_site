<% if false %>
  <div class="card card-inverse">
    <img class="card-img" src="<%= @banner_image.image %>" alt="Card image">
    <div class="card-img-overlay">
      <h4 class="card-title">Card title</h4>
      <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
      <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
    </div>
  </div>
<% end %>
<% if true %>
<div class="panel header-panel navbar-fixed-top navbar_row">
      <div class="panel-heading main_background_imagex" >
        <div  >
          <div class="col-md-4">
            <% if !@banner_image.nil? %>
              <%#= image_tag(@banner_image.image ) %>
            <% end %>
          </div>
          <div class="col-md-6">
            <%= render 'layouts/marque_bar' %>
          </div>
          <div class="col-md-2">
            <%= link_to 'Contact Us', contact_logs_path, {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#contactModal', :class=>'button_contact'} %>
          </div>
          <% if false && logged_in? %>
            <nav class="navbar navbar-inverse-x navbar-header" role="navigation" >
              <div class="col-md-1" ></div>
              <div class="col-md-10">
                <div class="container">
                  <div id="navbar" class="collapse navbar-collapse">
                    <ul class="nav navbar-nav">
                      <li class="navbar_text"><%= link_to 'Home', root_path %></li>
                      <% if !@pm_offerings.nil? && !@pm_offerings.empty? && !current_user.nil?%>
                        <li class="dropdown">
                          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            Our Methods & Tools<span class="caret"></span></a>
                          <ul class="dropdown-menu">
                            <% @pm_offerings.each do |pm| %>
                                <li><%= link_to pm.name, pm_path(:id => pm.id) %></li>
                            <% end %>
                          </ul>
                        </li>
                      <% end %>
                      <li><%= link_to 'Our Team', about_team_path %></li>
                      <% if !escent_main_pages.empty? || !community_web_pages.empty? %>
                        <li class="dropdown">
                          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            Our Community<span class="caret"></span></a>
                          <ul class="dropdown-menu">
                            <% escent_main_pages.each do |escent| %>
                                <li><a href=<%= escent.url %> target="_blank"><%=escent.name%></a></li>
                            <% end %>
                            <% if !escent_main_pages.empty? && !community_web_pages.empty? %>
                                <li role="separator" class="divider"></li>
                            <% end %>
                            <% community_web_pages.each do |web| %>
                                <li><a href=<%= web.url %> target="_blank"><%=web.name%></a></li>
                            <% end %>
                          </ul>
                        </li>
                      <% end %>
                      <% if admin_authorized? %>
                        <li class="dropdown">
                          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            Admin <span class="caret"></span></a>
                          <ul class="dropdown-menu">
                            <li><%= link_to 'Manage Users', admin_user_roles_path %></li>
                            <li><%= link_to 'Add User', sign_up_path %></li>
                            <li role="separator" class="divider"></li>
                            <li><%= link_to 'Manage Content', admin_offerings_path %></li>
                            <li role="separator" class="divider"></li>
                            <li><%= link_to 'Community Links', admin_escents_path %></li>
                            <li><%= link_to 'Captchas', admin_captchas_path %></li>
                            <li role="separator" class="divider"></li>
                            <li><%= link_to 'Reservations', prep_reservation_path %></li>
                            <li><%= link_to 'Events', prep_event_path %></li>
                            <li><%= link_to 'Venues', admin_prep_path %></li>
                            <li><%= link_to 'Pay Terms', event_terms_path %></li>
                          </ul>
                        </li>
                      <% end %>
                    </ul>
                    <ul class="nav navbar-nav ">
                      <% if current_user %>
                          <li><%= link_to ('Sign Out ' + current_user.first_name ), logout_path, method: :delete %></li>
                      <% else %>
                          <li><%= link_to 'Login', login_path %></li>
                      <% end -%>
                    </ul>
                  </div>
                </div>
      </div>
        </nav>
          <% end %>
        </div>
      </div>
    </div>
<% end %>
<%= render '/session/contact_us_modal' %>
<%= render '/session/career_modal' %>

