<div class="row">
  <div class="view_more"  data-toggle="collapse" data-target="#collapse_image<%= image.id %>"
        aria-expanded="false" aria-controls="collapse_image<%= image.id %>">
    View: <u><%= image.name %></u>
  </div>
  <div id="collapse_image<%= image.id %>" class="panel-collapse collapse" role="tabpanel"
       aria-labelledby="heading_image<%= image.id %>">
    <div class="col-md-11">
      <div class="panel-body offering_image_block">
        <div  class="row offering_image">
          <%= image.pic? ? image_tag(image.image) : '' %>
        </div>
        <% if !image.description.nil? && image.description != '' %>
          <div class="row offering_images_descript">
            <%=image.description.html_safe %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>