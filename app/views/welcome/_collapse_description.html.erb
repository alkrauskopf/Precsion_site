<% if !overview.description.empty? %>
  <span class="more"  data-toggle="collapse" data-target="#collapse<%= overview.id %>"
        aria-expanded="false" aria-controls="collapse<%= overview.id %>">
    More...
  </span>
  <div id="collapse<%= overview.id %>" class="panel-collapse collapse" role="tabpanel"
       aria-labelledby="heading<%= overview.id %>">
    <p><%= overview.description.html_safe %></p>
  </div>
<% end %>
