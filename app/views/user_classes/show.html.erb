<div class="page-header">
  <%= link_to user_classes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_user_class_path(@user_class), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show user_class</h1>
</div>

<dl class="dl-horizontal">
  <dt>Abbrev:</dt>
  <dd><%= @user_class.abbrev %></dd>

  <dt>Name:</dt>
  <dd><%= @user_class.name %></dd>

  <dt>Description:</dt>
  <dd><%= @user_class.description %></dd>

</dl>
